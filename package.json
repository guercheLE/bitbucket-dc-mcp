{
  "name": "bitbucket-dc-mcp",
  "version": "1.0.1",
  "description": "Bitbucket DataCenter MCP Server with semantic search and operation execution",
  "private": false,
  "type": "module",
  "main": "dist/index.js",
  "types": "dist/index.d.ts",
  "bin": {
    "bitbucket-dc-mcp": "./dist/cli.js"
  },
  "files": [
    "dist/",
    "README.md",
    "LICENSE",
    "data/embeddings.db",
    "data/operations.json",
    "data/schemas.json",
    "scripts/preinstall.js",
    "scripts/postinstall.js"
  ],
  "engines": {
    "node": ">=22.0.0"
  },
  "directories": {
    "doc": "docs"
  },
  "scripts": {
    "preinstall": "node scripts/preinstall.js",
    "postinstall": "node scripts/postinstall.js",
    "validate-embeddings": "node scripts/validate-embeddings.js",
    "prebuild": "npm run validate-embeddings",
    "build": "tsc",
    "postbuild": "chmod +x dist/cli.js",
    "prepublishOnly": "npm run validate-embeddings && npm run build && npm test",
    "dev": "tsc --watch",
    "test": "vitest run",
    "test:unit": "vitest run tests/unit",
    "test:integration": "RUN_INTEGRATION_TESTS=true vitest run tests/integration",
    "test:e2e": "RUN_E2E_TESTS=true vitest run tests/e2e",
    "test:e2e:real": "E2E_USE_REAL_BITBUCKET=true RUN_E2E_TESTS=true vitest run tests/e2e",
    "test:all": "RUN_INTEGRATION_TESTS=true RUN_E2E_TESTS=true vitest run",
    "test:benchmark": "vitest run tests/benchmarks",
    "test:coverage": "vitest run --coverage",
    "lint": "eslint \"src\" --ext .ts",
    "lint:fix": "eslint \"src\" --ext .ts --fix",
    "format": "prettier --write \"src/**/*.ts\" \"tests/**/*.ts\"",
    "format:check": "prettier --check \"src/**/*.ts\" \"tests/**/*.ts\"",
    "license:check": "tsx scripts/add-license-headers.ts --check",
    "license:fix": "tsx scripts/add-license-headers.ts --fix",
    "download-openapi": "tsx scripts/download-openapi.ts",
    "generate-schemas": "tsx scripts/generate-schemas.ts",
    "generate-embeddings": "tsx scripts/generate-embeddings.ts",
    "populate-db": "tsx scripts/populate-db.ts",
    "search": "tsx src/cli/search-test.ts",
    "setup": "tsx src/cli/setup-wizard.ts",
    "start": "node dist/index.js",
    "docker:build": "docker build -t bitbucket-dc-mcp:latest .",
    "docker:tag": "./scripts/docker-tag-and-push.sh",
    "prepare": "husky install",
    "semantic-release": "semantic-release"
  },
  "devDependencies": {
    "@semantic-release/changelog": "^6.0.3",
    "@semantic-release/commit-analyzer": "^13.0.1",
    "@semantic-release/git": "^10.0.1",
    "@semantic-release/github": "^12.0.0",
    "@semantic-release/npm": "^13.1.1",
    "@semantic-release/release-notes-generator": "^14.1.0",
    "@types/better-sqlite3": "^7.6.13",
    "@types/keytar": "^4.4.0",
    "@types/node": "^22.10.0",
    "@typescript-eslint/eslint-plugin": "^8.17.0",
    "@typescript-eslint/parser": "^8.17.0",
    "@vitest/coverage-v8": "^3.2.4",
    "conventional-changelog-conventionalcommits": "^9.1.0",
    "eslint": "^9.38.0",
    "globals": "^16.4.0",
    "husky": "^9.1.7",
    "node-fetch": "^3.3.2",
    "openapi3-ts": "^4.5.0",
    "prettier": "^3.6.2",
    "semantic-release": "^25.0.0",
    "testcontainers": "^11.7.1",
    "tsx": "^4.20.6",
    "typescript": "^5.1.6",
    "vitest": "^3.2.4"
  },
  "repository": {
    "type": "git",
    "url": "git+https://github.com/guercheLE/bitbucket-dc-mcp.git"
  },
  "keywords": [
    "bitbucket",
    "mcp",
    "model-context-protocol",
    "semantic-search",
    "cli",
    "bitbucket-datacenter"
  ],
  "author": "",
  "license": "LGPL-3.0-or-later",
  "bugs": {
    "url": "https://github.com/guercheLE/bitbucket-dc-mcp/issues"
  },
  "homepage": "https://github.com/guercheLE/bitbucket-dc-mcp#readme",
  "dependencies": {
    "@modelcontextprotocol/sdk": "^1.20.1",
    "@types/express": "^5.0.3",
    "@types/inquirer": "^9.0.9",
    "@types/js-yaml": "^4.0.9",
    "@xenova/transformers": "^2.17.1",
    "better-sqlite3": "^12.4.1",
    "chalk": "^5.3.0",
    "cli-table3": "^0.6.5",
    "commander": "^14.0.1",
    "express": "^5.1.0",
    "inquirer": "^12.10.0",
    "js-yaml": "^4.1.0",
    "keytar": "^7.9.0",
    "oauth-1.0a": "^2.2.6",
    "open": "^10.2.0",
    "pino": "^10.1.0",
    "pino-roll": "^1.2.0",
    "sqlite-vec": "^0.1.6",
    "zod": "^3.25.76",
    "zod-to-json-schema": "^3.24.6"
  }
}
